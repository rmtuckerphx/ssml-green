    "SSML_1.0": {
        "summary›supported elements›<speak>": "<speak>\n   Simple text to speech.\n</speak>",
        "summary›supported elements›<p>": "<speak>\n  <p>\n    <s>This is the first sentence of the paragraph</s>\n    <s>Here's another sentence.</s>\n  </p>\n</speak>",
        "summary›supported elements›<s>": "<speak>\n  <p>\n    <s>This is the first sentence of the paragraph</s>\n    <s>Here's another sentence.</s>\n  </p>\n</speak>",
        "summary›supported elements›<say-as>": "<speak>\n    Here is a number spoken as a cardinal number: \n    <say-as interpret-as=\"cardinal\">12345</say-as>.\n    Here is the same number with each digit spoken separately:\n    <say-as interpret-as=\"digits\">12345</say-as>.\n    Here is a word spelled out: <say-as interpret-as=\"spell-out\">hello</say-as>\n</speak>\nspeak\n",
        "summary›supported elements›<phoneme>": "<speak>\n    You say, <phoneme alphabet=\"ipa\" ph=\"pɪˈkɑːn\">pecan</phoneme>. \n    I say, <phoneme alphabet=\"ipa\" ph=\"ˈpi.kæn\">pecan</phoneme>.\n</speak>\n",
        "summary›supported elements›<prosody>": "<speak>\n    Normal volume for the first sentence.\n    <prosody volume=\"x-loud\">Louder volume for the second sentence</prosody>.\n    When I wake up, <prosody rate=\"x-slow\">I speak quite slowly</prosody>.\n    I can speak with my normal pitch, \n    <prosody pitch=\"x-high\"> but also with a much higher pitch </prosody>, \n    and also <prosody pitch=\"low\">with a lower pitch</prosody>.\n</speak>",
        "summary›supported elements›<sub>": "<speak>\n  <sub alias=\"Speech Synthesis Markup Language\">SSML</sub>\n</speak>",
        "summary›supported elements›<break>": "<speak>\n    There is a three second pause here <break time=\"3s\"/> \n    then the speech continues.\n</speak> ",
        "summary›supported elements›<emphasis>": "<speak>\n  That is a <emphasis> big </emphasis> car!\n  That is a <emphasis level=\"strong\"> huge </emphasis>\n  bank account!\n</speak>",
        "summary›supported elements›<audio>": "<speak>\n    Just wanted to say\n    <audio src=\"https://someurl.com/audio/welcome.mp3\" /> \n</speak> ",
        "summary›supported elements›<mark>": "<speak>\n    Go from <mark name=\"here\"/> here, to <mark name=\"there\"/> there! \n</speak> ",
        "summary›supported elements›<desc>": "<speak>\n    Just wanted to say\n    <audio src=\"https://someurl.com/audio/welcome.mp3\">\n      <desc>The word welcome</desc>\n    </audio>\n </speak> ",
        "summary›supported elements›<lexicon>": "<speak version=\"1.0\"\n       xmlns=\"http://www.w3.org/2001/10/synthesis\"\n       xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n       xsi:schemaLocation=\"http://www.w3.org/2001/10/synthesis\n                 http://www.w3.org/TR/speech-synthesis/synthesis.xsd\"\n       xml:lang=\"en-US\">\n\n  <lexicon uri=\"http://www.example.com/lexicon.file\"/>\n  <lexicon uri=\"http://www.example.com/strange-words.file\"\n           type=\"media-type\"/>\n  ...\n</speak>",
        "summary›supported elements›<meta>": "<?xml version=\"1.0\"?>\n<!DOCTYPE speak PUBLIC \"-//W3C//DTD SYNTHESIS 1.0//EN\"\n                  \"http://www.w3.org/TR/speech-synthesis/synthesis.dtd\">\n<speak version=\"1.0\"\n       xmlns=\"http://www.w3.org/2001/10/synthesis\"\n       xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n       xsi:schemaLocation=\"http://www.w3.org/2001/10/synthesis\n                 http://www.w3.org/TR/speech-synthesis/synthesis.xsd\"\n       xml:lang=\"en-US\">\n\n       <meta name=\"seeAlso\" content=\"http://example.com/my-ssml-metadata.xml\"/>\n       <meta http-equiv=\"Cache-Control\" content=\"no-cache\"/>\n\n</speak>",
        "summary›supported elements›<metadata>": "<?xml version=\"1.0\"?>\n<!DOCTYPE speak PUBLIC \"-//W3C//DTD SYNTHESIS 1.0//EN\"\n                  \"http://www.w3.org/TR/speech-synthesis/synthesis.dtd\">\n<speak version=\"1.0\"\n       xmlns=\"http://www.w3.org/2001/10/synthesis\"\n       xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n       xsi:schemaLocation=\"http://www.w3.org/2001/10/synthesis\n                 http://www.w3.org/TR/speech-synthesis/synthesis.xsd\"\n       xml:lang=\"en-US\">\n    \n  <metadata>\n   <rdf:RDF\n       xmlns:rdf = \"http://www.w3.org/1999/02/22-rdf-syntax-ns#\"\n       xmlns:rdfs = \"http://www.w3.org/2000/01/rdf-schema#\"\n       xmlns:dc = \"http://purl.org/dc/elements/1.1/\">\n\n   <!-- Metadata about the synthesis document -->\n   <rdf:Description rdf:about=\"http://www.example.com/meta.ssml\"\n       dc:Title=\"Hamlet-like Soliloquy\"\n       dc:Description=\"Aldine's Soliloquy in the style of Hamlet\"\n       dc:Publisher=\"W3C\"\n       dc:Language=\"en-US\"\n       dc:Date=\"2002-11-29\"\n       dc:Rights=\"Copyright 2002 Aldine Turnbet\"\n       dc:Format=\"application/ssml+xml\" >                \n       <dc:Creator>\n          <rdf:Seq ID=\"CreatorsAlphabeticalBySurname\">\n             <rdf:li>William Shakespeare</rdf:li>\n             <rdf:li>Aldine Turnbet</rdf:li>\n          </rdf:Seq>\n       </dc:Creator>\n   </rdf:Description>\n  </rdf:RDF>\n </metadata>\n\n</speak>",
        "summary›supported elements›<voice>": "<speak>\n  <voice gender=\"female\">Mary had a little lamb,<\/voice>\n\n  <!-- now request a different female child's voice -->\n  <voice gender=\"female\" variant=\"2\">\n    Its fleece was white as snow.\n  <\/voice>\n\n  <!-- processor-specific voice selection -->\n  <voice name=\"Mike\">I want to be like Mike.<\/voice>\n<\/speak>",

